DOCKER_HOME=${HOME}/chakio_dockerfiles
DOCKER_ROS_HOME=${DOCKER_HOME}/ros_mode
DOCKER_HSR_HOME=${DOCKER_HOME}/hsr_mode
DOCKER_MASTER_HOME=${DOCKER_HOME}/ros_master
DOCKER_GPy_HOME=${DOCKER_HOME}/GPy
DOCKER_GPy3_HOME=${DOCKER_HOME}/GPy3
alias ros_mode="cd ${DOCKER_ROS_HOME} && ./run.sh"
alias hsr_mode="cd ${DOCKER_HSR_HOME} && ./run.sh"
alias master_mode="cd ${DOCKER_MASTER_HOME} && ./master_run.sh"
alias sync_mode="cd ${DOCKER_MASTER_HOME} && ./sync_run.sh"
alias node_manager="cd ${DOCKER_MASTER_HOME} && ./manager_run.sh"
alias gpy="cd ${DOCKER_GPy_HOME} && ./run.sh"
alias gpy3="cd ${DOCKER_GPy3_HOME} && ./run.sh"